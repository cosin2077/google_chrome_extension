{
    "manifest_version": 2,
    "name": "Source code helper",
    "version": "0.1.0",
    "description": "Help you read github source code better",
    "icons": {
        "16": "images/code16.png",
        "48": "images/code48.png",
        "128": "images/code128.png"
    },
    "permissions": [
        "https://github.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["*://github.com/*","*://www.github.com/*"],
            "js": ["js/code.js"]
        }
    ]
}