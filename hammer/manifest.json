{
    "manifest_version": 2,
    "version": "1.9.1",
    "name": "hammer",
    "description": "Chrome useful tools",
    "default_locale": "en",
    "icons": {
        "16": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/icon_128x128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/logo.png",
            "38": "images/logo.png"
        },
        "default_title": "hammer"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.googletagmanager.com/ https://www.google-analytics.com/; object-src 'self'",
	"short_name": "hammer",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/hammer.js"]
        }
    ]
}